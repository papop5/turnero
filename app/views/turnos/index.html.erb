<p id="notice"><%= notice %></p>

<h1>Listing Turnos</h1>

<table>
  <thead>
    <tr>
      <th>Usuario ingreso</th>
      <th>Usuario atencion</th>
      <th>Agencia</th>
      <th>Nombres</th>
      <th>Asunto</th>
      <th>Descripcion</th>
      <th>Hora ingreso</th>
      <th>Hora atencion</th>
      <th>Hora finalizacion</th>
      <th>Comentario</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @turnos.each do |turno| %>
      <tr>
        <td><%= turno.usuario_ingreso_id %></td>
        <td><%= turno.usuario_atencion_id %></td>
        <td><%= turno.agencia %></td>
        <td><%= turno.nombres %></td>
        <td><%= turno.asunto %></td>
        <td><%= turno.descripcion %></td>
        <td><%= turno.hora_ingreso %></td>
        <td><%= turno.hora_atencion %></td>
        <td><%= turno.hora_finalizacion %></td>
        <td><%= turno.comentario %></td>
        <td><%= turno.estado %></td>
        <td><%= link_to 'Show', turno %></td>
        <td><%= link_to 'Edit', edit_turno_path(turno) %></td>
        <td><%= link_to 'Destroy', turno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Turno', new_turno_path %>
